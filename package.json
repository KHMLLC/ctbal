{
  "name": "ctbal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "hardhat run scripts/test-analytics.ts",
    "test:compile": "hardhat compile --force",
    "compile": "hardhat compile",
    "deploy": "npx tsx scripts/deploy-viem-direct.ts",
    "analytics": "hardhat run scripts/test-analytics.ts",
    "summary": "hardhat run scripts/review-summary.ts",
    "demo": "hardhat run scripts/system-overview.ts",
    "networks": "hardhat run scripts/network-analysis.ts",
    "clean": "hardhat clean",
    "deploy:local": "hardhat run scripts/deploy-production.ts --network localhost",
    "deploy:khmweb01": "hardhat run scripts/deploy-production.ts --network khmweb01",
    "deploy:sepolia": "npx tsx scripts/deploy-viem-direct.ts",
    "deploy:quorum": "hardhat run scripts/deploy-production.ts --network quorum",
    "deploy:prod": "npm run deploy:sepolia",
    "verify:sepolia": "echo 'Run verification commands from deployment output'",
    "node:local": "hardhat node",
    "setup:env": "Copy-Item .env.example .env -Force && Write-Host 'Please edit .env with your credentials'",
    "validate:setup": "hardhat run scripts/validate-setup.ts --network sepolia",
    "check:deployment": "hardhat run scripts/deployment-check.ts",
    "import:csv": "npx tsx scripts/import-wyoming-sepolia.ts",
    "queue:add": "npx tsx scripts/csv-queue-manager.ts add",
    "queue:list": "npx tsx scripts/csv-queue-manager.ts list",
    "queue:process": "npx tsx scripts/csv-queue-manager.ts process",
    "queue:process-all": "npx tsx scripts/csv-queue-manager.ts process-all",
    "queue:status": "npx tsx scripts/csv-queue-manager.ts status",
    "queue:clear": "npx tsx scripts/csv-queue-manager.ts clear",
    "queue:remove": "npx tsx scripts/csv-queue-manager.ts remove",
    "scrape:transfer": "python scripts/ctbal-scrape-integration.py transfer",
    "scrape:full-pipeline": "python scripts/ctbal-scrape-integration.py full",
    "import:csv:khmweb01": "hardhat run scripts/batch-csv-import.ts --network khmweb01",
    "import:csv:dry": "powershell -c \"$env:DRY_RUN='true'; hardhat run scripts/batch-csv-import.ts\"",
    "analyze:csv": "hardhat run scripts/test-csv-analysis.ts",
    "verify:sepolia:contracts": "npx tsx scripts/verify-sepolia-deployment.ts",
    "query:sepolia:analytics": "npx tsx scripts/query-sepolia-analytics.ts",
    "import:wyoming:sepolia": "npx tsx scripts/import-wyoming-sepolia.ts",
    "dashboard:sepolia": "npx tsx scripts/sepolia-dashboard.ts",
    "report:comprehensive": "npx tsx scripts/comprehensive-sepolia-report.ts",
    "success:summary": "npx tsx scripts/success-summary.ts",
    "production:ready": "npm run compile && npm run test && npm run analytics",
    "infrastructure:status": "npx tsx scripts/infrastructure-status.ts",
    "infrastructure:deploy": "npm run compile && npm run deploy:sepolia && npm run verify:sepolia",
    "infrastructure:monitor": "npx tsx scripts/infrastructure-monitor.ts",
    "deployment:readiness": "npx tsx scripts/deployment-readiness.ts",
    "security:audit": "npm audit --audit-level=high",
    "docs:generate": "npx tsx scripts/generate-docs.ts",
    "system:health": "npm run queue:status && npm run dashboard:sepolia",
    "backup:create": "npx tsx scripts/backup-system-state.ts",
    "git:prepare": "git add . && git status",
    "release:prepare": "npm run production:ready && npm run docs:generate && npm run git:prepare"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^4.0.3",
    "@nomicfoundation/hardhat-ignition": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^3.0.2",
    "@nomicfoundation/hardhat-toolbox-viem": "^5.0.1",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.1",
    "chai": "^6.2.1",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "forge-std": "github:foundry-rs/forge-std#v1.9.4",
    "hardhat": "^3.0.14",
    "mocha": "^11.7.5",
    "typescript": "~5.8.0",
    "viem": "^2.39.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "csv-parser": "^3.2.0",
    "web3": "^4.16.0"
  }
}
